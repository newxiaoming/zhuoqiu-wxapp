.userinfo{
  width: 16.66%;
}
.cathecticList text{
  width: 16.66%;
}
.cathecticList text:nth-child(2), .cathecticList text:nth-child(3){
  text-align: left;
}
.cathecticList text:last-child, .cathecticList text:nth-child(4){
  width: 25%;
} 
.gatherAmount, .payAmount{
  color: #333 !important;
}
.gatherAmount text, .payAmount text{
  font-weight: bold;
}
.shareBtn{
  position: fixed;
  bottom: 0;  
  width: 100%;
  height: 98rpx;
  line-height: 98rpx;  
  background-color: #f85656;
  text-align: center;
}
.shareBtn text{
  color: #fff;
}